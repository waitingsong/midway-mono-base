{
  "name": "@scope/docs",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "bin": {},
  "types": "src/index.ts",
  "imports": {
    "#package.json": "./package.json",
    "##/*": "./src/*",
    "#a/*": "./src/app/*",
    "#@/*": "./test/*"
  },
  "exports": {
    ".": "./dist/index.js",
    "./v1": "./dist/v1/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@midwayjs/validate": "^3.14.0",
    "@waiting/shared-types": "^23.0.0"
  },
  "devDependencies": {
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "files": [
    "README.*.md",
    "Dockerfile",
    "npm-shrinkwrap.json",
    "tsconfig.json",
    "tsconfig.base.json",
    "asset",
    "src/**/*.js",
    "src/**/*.ts",
    "bin",
    "database",
    "dist",
    "!**/*.spec.*",
    "!**/*.tsbuildinfo"
  ],
  "homepage": "",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://"
  },
  "license": "",
  "scripts": {
    "build": "npm run tsc && tsc-alias && npm run db:gen",
    "clean": "npm run clean:lock && npm run clean:dist && npm run clean:log",
    "clean:cache": "rm -rf .eslintcache .tsbuildinfo",
    "clean:dist": "npm run clean:cache && rm -rf dist/*",
    "clean:lock": "rm -f package-lock.json",
    "clean:log": "rm -rf ./logs ./run",
    "cov": "c8 --all npm run test -- --parallel=false",
    "db:gen": "kmore gen --path ./src",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=test/tsconfig.json mocha --jobs=4",
    "tsc": "tsc -b"
  }
}
