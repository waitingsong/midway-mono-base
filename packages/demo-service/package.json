{
  "name": "@scope/demo-service",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "bin": {},
  "dependencies": {
    "@scope/docs": "^0.0.0",
    "@midwayjs/bootstrap": "^3.3.0",
    "@midwayjs/decorator": "^3.3.0",
    "@midwayjs/koa": "^3.3.0",
    "@midwayjs/prometheus": "^3.3.0",
    "@midwayjs/redis": "^3.3.0",
    "@midwayjs/swagger": "^3.3.0",
    "@midwayjs/validate": "^3.3.0",
    "@mw-components/fetch": "^19.0.1",
    "@mw-components/jaeger": "^11.0.8",
    "@mw-components/jwt": "^11.0.10",
    "@mw-components/kmore": "^26.0.0",
    "@mw-components/koid": "^9.0.0",
    "@mw-components/taskman": "^16.4.2",
    "@waiting/base64": "^4.2.9",
    "@waiting/shared-core": "^18.0.0",
    "@waiting/shared-types": "^18.0.0",
    "pg": "^8.7.3",
    "rxjs": "^7.5.0"
  },
  "devDependencies": {
    "cross-env": "7"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "files": [
    "dist",
    "bootstrap.js",
    "Dockerfile",
    "tsconfig.json",
    "tsconfig.cjs.json"
  ],
  "homepage": "",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://"
  },
  "license": "",
  "scripts": {
    "build": "midway-bin build && npm run db:gen",
    "clean": "npm run clean:cache & npm run clean:mw",
    "clean:cache": "rm .eslintcache -f",
    "clean:lock": "rm package-lock.json -f",
    "clean:mw": "midway-bin clean",
    "cov": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=test/tsconfig.json c8 mocha --parallel=false",
    "cov:local": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=test/tsconfig.json c8 ../../node_modules/.bin/mocha --parallel=false",
    "db:gen": "kmore gen --project tsconfig.cjs.json --path ./src",
    "debug": "cross-env NODE_ENV=local ../../node_modules/.bin/midway-bin dev --debug --ts",
    "dev": "cross-env NODE_ENV=local ../../node_modules/.bin/midway-bin dev --ts --fast --port=7001",
    "dev:debug": "cross-env NODE_ENV=local NODE_DEBUG=midway* ../../node_modules/.bin/midway-bin dev --ts",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "purge": "npm run clean && rm node_modules -rf",
    "start": "node bootstrap.js",
    "start_build": "npm run clean && npm run build && cross-env NODE_ENV=development midway-bin dev --ts",
    "stop": "egg-scripts stop --title=midway-server-svc",
    "test": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_PROJECT=test/tsconfig.json mocha --parallel=false",
    "test:local": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_PROJECT=test/tsconfig.json ../../node_modules/.bin/mocha --parallel=false",
    "tsc": "tsc -p tsconfig.json"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    ".vscode/.tsbuildinfo.*",
    ".vscode/.jest-cache",
    "dist",
    "logs",
    "run"
  ],
  "midway-cli": {
    "experimentalFeatures": {
      "ignoreTsError": true
    }
  },
  "midway-bin-build": {
    "include": [
      "app/public",
      "app/view"
    ]
  }
}
