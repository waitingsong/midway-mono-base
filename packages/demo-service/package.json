{
  "name": "@scope/demo-service",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "bin": {},
  "dependencies": {
    "@mwcp/boot": "^10.2.8",
    "@mwcp/taskman": "^40.1.1",
    "@scope/docs": "^0.0.0",
    "pg-native": "^3.0.1"
  },
  "devDependencies": {
    "cross-env": "7"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "files": [
    "npm-shrinkwrap.json",
    "tsconfig.json",
    "Dockerfile",
    "bootstrap.js",
    "dist"
  ],
  "homepage": "",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://"
  },
  "license": "",
  "scripts": {
    "build": "midway-bin build && npm run db:gen-cjs && tsc-alias",
    "clean": "npm run clean:cache & npm run clean:mw & rm -rf dist/*",
    "clean:cache": "rm -rf .eslintcache .tsbuildinfo",
    "clean:lock": "rm package-lock.json -f",
    "clean:mw": "midway-bin clean",
    "cov": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=test/tsconfig.json c8 mocha --parallel=false",
    "cov:local": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=test/tsconfig.json c8 ../../node_modules/.bin/mocha --parallel=false",
    "db:gen": "kmore gen --project tsconfig.cjs.json --path ./src",
    "db:gen-cjs": "kmore gen --project tsconfig.cjs.json --path ./src --format cjs",
    "debug": "cross-env MIDWAY_SERVER_ENV=local ../../node_modules/.bin/midway-bin dev -ts --fast --debug",
    "dev": "cross-env MIDWAY_SERVER_ENV=local ../../node_modules/.bin/midway-bin dev --ts --fast --port=7001",
    "dev:debug": "cross-env MIDWAY_SERVER_ENV=local NODE_DEBUG=midway* ../../node_modules/.bin/midway-bin dev --ts",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "purge": "npm run clean && rm node_modules -rf && rm package-lock.json -f",
    "pretest": "npm run build",
    "pretest:local": "npm run build",
    "start": "node bootstrap.js",
    "start_build": "npm run clean && npm run build && cross-env NODE_ENV=development midway-bin dev --ts",
    "test": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_PROJECT=test/tsconfig.json mocha --parallel=false",
    "test:local": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_PROJECT=test/tsconfig.json ../../node_modules/.bin/mocha --parallel=false",
    "tsc": "tsc -b "
  },
  "midway-bin-clean": [
    ".tsbuildinfo",
    ".vscode/.jest-cache",
    "dist",
    "logs",
    "run"
  ],
  "midway-cli": {
    "experimentalFeatures": {
      "ignoreTsError": true
    }
  },
  "midway-bin-build": {
    "include": [
      "app/public",
      "app/view"
    ]
  }
}
