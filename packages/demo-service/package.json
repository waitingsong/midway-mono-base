{
  "name": "@scope/demo-service",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "bin": {},
  "dependencies": {
    "@mwcp/boot": "^11.3.2",
    "@mwcp/taskman": "^41.6.2",
    "@scope/docs": "^0.0.0",
    "pg": "^8.11.1"
  },
  "devDependencies": {
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "files": [
    "README.*.md",
    "Dockerfile",
    "bootstrap.js",
    "npm-shrinkwrap.json",
    "tsconfig.json",
    "tsconfig.base.json",
    "assert",
    "dist"
  ],
  "homepage": "",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://"
  },
  "license": "",
  "scripts": {
    "build": "midway-bin build && npm run db:gen-cjs && tsc-alias",
    "clean": "npm run clean:lock && npm run clean:cache && npm run clean:dist && npm run clean:log",
    "clean:cache": "rm -rf .eslintcache .tsbuildinfo .vscode/.jest-cache",
    "clean:dist": "rm -rf dist/* .tsbuildinfo",
    "clean:lock": "rm package-lock.json -f",
    "clean:log": "rm -rf ./logs ./run",
    "cov": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=test/tsconfig.json c8 mocha --parallel=false",
    "db:gen": "kmore gen --project tsconfig.cjs.json --path ./src",
    "db:gen-cjs": "kmore gen --project tsconfig.cjs.json --path ./src --format cjs",
    "debug": "cross-env MIDWAY_SERVER_ENV=local ../../node_modules/.bin/midway-bin dev -ts --fast --debug",
    "dev": "cross-env MIDWAY_SERVER_ENV=local ../../node_modules/.bin/midway-bin dev --ts --fast --port=7001",
    "dev:debug": "cross-env MIDWAY_SERVER_ENV=local NODE_DEBUG=midway* ../../node_modules/.bin/midway-bin dev --ts",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "predev": "rm -rf node_modules/.midway_bin_cache",
    "predev:debug": "rm -rf node_modules/.midway_bin_cache",
    "pretest": "npm run build",
    "pretest:local": "npm run build",
    "purge": "npm run clean && rm node_modules -rf && rm package-lock.json -f",
    "start": "node bootstrap.js",
    "start_build": "npm run clean && npm run build && cross-env NODE_ENV=development midway-bin dev --ts",
    "test": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_PROJECT=test/tsconfig.json mocha ",
    "test:local": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_PROJECT=test/tsconfig.json ../../node_modules/.bin/mocha ",
    "tsc": "tsc -b "
  },
  "midway-cli": {
    "experimentalFeatures": {
      "ignoreTsError": true
    }
  },
  "midway-bin-build": {
    "include": [
      "app/public",
      "app/view"
    ]
  }
}
