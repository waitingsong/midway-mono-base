{
  "name": "@scope/demo-service",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "bin": {},
  "dependencies": {
    "@scope/docs": "^0.0.0",
    "@midwayjs/bootstrap": "^2.13.0",
    "@midwayjs/decorator": "^2.13.0",
    "@midwayjs/prometheus": "^2.13.0",
    "@midwayjs/redis": "^2.13.0",
    "@midwayjs/swagger": "^1.3.0",
    "@midwayjs/web": "^2.13.0",
    "@mw-components/fetch": "^15.0.2",
    "@mw-components/jaeger": "^9.0.2",
    "@mw-components/jwt": "^9.2.1",
    "@mw-components/kmore": "^23.0.2",
    "@mw-components/koid": "^5.0.0",
    "@mw-components/taskman": "^13.0.5",
    "@waiting/base64": "^4.2.9",
    "@waiting/shared-core": "^14.4.0",
    "@waiting/shared-types": "^14.4.0",
    "egg": "^2.31.0",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.15.1",
    "pg": "^8.7.1",
    "rxjs": "^7.4.0",
    "tsconfig-paths": "3"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.2.88",
    "@midwayjs/koa": "^2.13.0",
    "@midwayjs/mock": "^2.13.0",
    "@types/rewire": "2",
    "@types/supertest": "^2.0.11",
    "cross-env": "7",
    "kmore-types": "^23.0.2",
    "power-assert": "1",
    "rewire": "5",
    "supertest": "^6.1.3",
    "swagger-ui-dist": "^3.49.0"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "files": [
    "dist",
    "Dockerfile",
    "tsconfig.json",
    "tsconfig.cjs.json"
  ],
  "homepage": "",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://"
  },
  "license": "",
  "scripts": {
    "build": "midway-bin build && npm run db:gen",
    "clean": "npm run clean:cache & npm run clean:mw",
    "clean:cache": "rm .eslintcache -f",
    "clean:lock": "rm package-lock.json -f",
    "clean:mw": "midway-bin clean",
    "cov": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=test/tsconfig.json c8 mocha --parallel=false",
    "cov:local": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=test/tsconfig.json c8 ../../node_modules/.bin/mocha --parallel=false",
    "db:gen": "kmore gen --project tsconfig.cjs.json --path ./src",
    "debug": "cross-env NODE_ENV=local ../../node_modules/.bin/midway-bin dev --debug --ts",
    "dev": "cross-env NODE_ENV=local TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true ../../node_modules/.bin/midway-bin dev --ts --port=7001",
    "dev:debug": "cross-env NODE_ENV=local TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true NODE_DEBUG=midway* ../../node_modules/.bin/midway-bin dev --ts",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "purge": "npm run clean && rm node_modules -rf",
    "start": "egg-scripts start --daemon --framework=@midwayjs/web --title=midway-server-svc --workers=4",
    "start_build": "npm run clean && npm run build && cross-env NODE_ENV=development midway-bin dev --ts",
    "start_docker": "egg-scripts start --framework=@midwayjs/web --title=midway-server-svc",
    "stop": "egg-scripts stop --title=midway-server-svc",
    "test": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_PROJECT=test/tsconfig.json mocha --parallel=false",
    "test:local": "cross-env MIDWAY_SERVER_ENV=unittest TS_NODE_PROJECT=test/tsconfig.json ../../node_modules/.bin/mocha --parallel=false",
    "tsc": "tsc -p tsconfig.json"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    ".vscode/.tsbuildinfo.*",
    ".vscode/.jest-cache",
    "dist",
    "logs",
    "run"
  ],
  "midway-bin-build": {
    "include": [
      "app/public",
      "app/view"
    ]
  }
}
