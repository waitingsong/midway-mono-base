{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Midway TEST",
      "type": "node",
      "console": "internalConsole",
      "env": {
        "NODE_ENV": "unittest",
        "POSTGRES_HOST": "",
        "POSTGRES_PORT": "5432",
        "POSTGRES_DB": "",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "",
      },
      "request": "launch",
      "restart": false,
      "runtimeArgs": [
        "run",
        "test:local",
        "--",
        "--runInBand",
        "--preserve-symlinks",
        "--inspect-brk"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "**/node_modules/rxjs/**",
        "**/node_modules/mocha/**",
        "**/async_hooks.js",
        "**/*async_hook*.js",
        "**/node_modules/@midwayjs/core/dist/service/middlewareService.js",
        "<node_internals>/**",
        "<node_internals>/async_hooks",
        "<node_internals>/internal/async_hooks.js",
        "<node_internals>/**/*.js"
      ],
      "smartStep": false,
      "timeout": 30000
    },
    {
      "name": "Midway Debug",
      "type": "node",
      "autoAttachChildProcesses": true,
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "local",
        "TS_NODE_TYPE_CHECK": "false",
        "TS_NODE_TRANSPILE_ONLY": "true",
        "POSTGRES_HOST": "",
        "POSTGRES_PORT": "5432",
        "POSTGRES_DB": "",
        "POSTGRES_USER": "",
        "POSTGRES_PASSWORD": "",
      },
      "request": "launch",
      "restart": true,
      "runtimeArgs": [
        "run",
        "debug",
        "--",
        "--preserve-symlinks",
        "--inspect-brk"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "**/node_modules/**",
        "**/node_modules/rxjs/**",
        "**/async_hooks.js",
        "<node_internals>/**",
        "<node_internals>/**/*.js"
      ],
      "smartStep": true,
      "timeout": 30000
    }
  ]
}
